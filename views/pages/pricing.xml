                </div>
            </t>
        </template>

        <record id="pricing_page_link" model="website.menu">
            <field name="name">Pricing</field>
            <field name="url">/page/pricing</field>
            <field name="parent_id" ref="website.main_menu" />
            <field name="sequence" type="int">52</field>
        </record>

    </data>
</odoo>