<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <!-- === Pricing Page === -->
        <template name="Precios" id="website.pricing" page="True">
            <t t-call="website.layout">
                <div id="wrap">
                    <!--page title start-->
                    <section class="page-title page-title-center cover-pricing padding-top-220 padding-bottom-120 overlay purple-5 fixed-bg">
