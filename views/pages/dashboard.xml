<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template name="Precios" id="website.dashboard" page="True">
            <t t-call="website.layout">
                <div id="wrap">
                        <!--page title start-->
                    <section class="page-title ptb-140">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    <h2 class="text-bold white-text">Panel de control</h2>
                                    <ol class="breadcrumb">
                                        <li><a href="/">Inicio</a></li>
                                        <li class="active">Panel de control</li>
                                    </ol>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!--page title end-->

                        <section class="section-padding gray-bg">
                            <div class="container">

                                <div class="login-wrapper">
                                    <div class="card-wrapper"></div>
                                    <div class="card-wrapper">
                                        <h1 class="title">Dashboard</h1>
                                    </div>
                                </div>
                            </div>
                        </section>
                        
                </div>
            </t>
        </template>

        <record id="dashboard_page_link" model="website.menu">
            <field name="name">Dashboard</field>
            <field name="url">/page/dashboard</field>
            <field name="parent_id" ref="website.main_menu" />
            <field name="sequence" type="int">53</field>
        </record>
    </data>
</odoo>