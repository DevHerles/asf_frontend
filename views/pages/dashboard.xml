                                </div>
                                </div>
                            </div>
                            </div>
                        </div>
                        <!--end container-->
                        </section>

                </div>
            </t>
        </template>

        <record id="dashboard_page_link" model="website.menu">
            <field name="name">Dashboard</field>
            <field name="url">/page/dashboard</field>
            <field name="parent_id" ref="website.main_menu" />
            <field name="sequence" type="int">49</field>
        </record>
    </data>
</odoo>